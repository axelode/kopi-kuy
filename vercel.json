{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/shop",
            "dest": "/dist/index.html"
        },
        {
            "src": "/cart",
            "dest": "/dist/index.html"
        },
        {
            "src": "/",
            "dest": "/dist/index.html"
        },
        {
            "src": "/(.*)",
            "dest": "/dist/index.html"
        }
    ]
}